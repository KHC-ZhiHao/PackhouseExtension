{
	"name": "packhouse",
	"displayName": "Packhouse",
	"publisher": "khczhihao",
	"description": "",
	"version": "0.0.7",
	"repository": {
		"type": "git",
		"url": "https://github.com/KHC-ZhiHao/PackhouseVSCodeExtension"
	},
	"license": "SEE LICENSE IN LICENSE.txt",
	"icon": "assets/icon.png",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains: **/.packhouse"
	],
	"main": "./out/extension.js",
	"contributes": {
		"snippets": [
			{
				"language": "javascript",
				"path": "./snippets.json"
			},
			{
				"language": "typescript",
				"path": "./snippets.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"publish": "vsce publish"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/node": "^12.11.7",
		"@types/mocha": "^5.2.7",
		"@types/vscode": "^1.41.0",
		"mocha": "^10.2.0",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"glob": "^7.1.6",
		"typescript": "^3.7.4"
	}
}
